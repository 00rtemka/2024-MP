class Radar(position):
    _position =position
    def __new__(cls):
          if cls._position is None:
              cls._position = super(Radar, cls).__new__(cls)
          return cls._position
    def __init__:
          pass

class Vector:
    def __init__(self, x, y, z):
        self.x = x
        self.y = y
        self.z = z
